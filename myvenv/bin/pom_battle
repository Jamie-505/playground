#!/home/james/dev/playground/myvenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pommerman==0.2.0','console_scripts','pom_battle'
__requires__ = 'pommerman==0.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pommerman==0.2.0', 'console_scripts', 'pom_battle')()
    )
